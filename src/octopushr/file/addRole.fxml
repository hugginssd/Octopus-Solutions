<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="octopushr.file.AddRoleController">

    <stylesheets>
        <URL value="@/resources/css/addrole.css" />
    </stylesheets>
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="70.0" prefWidth="270.0" text="User Role" textFill="#f5f3f3">
            <font>
                <Font name="Arial Rounded MT Bold" size="26.0" />

            </font>
        </Label>
        <TextField fx:id="txtUserRole" layoutX="20.0" layoutY="150.0" prefHeight="32.0" prefWidth="232.0" promptText="User Role" />
        <TextField fx:id="txtId" layoutX="20.0" layoutY="110.0" prefHeight="32.0" prefWidth="232.0" promptText="Id" />
        <Button fx:id="btnAddRole" layoutX="20.0" layoutY="188.0" mnemonicParsing="false" onAction="#addRole" prefHeight="30.0" prefWidth="72.0" text="ADD" />
        <Button fx:id="btnCloseForm" layoutX="100.0" layoutY="188.0" mnemonicParsing="false" onAction="#closeThisStage" prefHeight="30.0" prefWidth="103.0" text="CLOSE FORM" />

        <ListView fx:id="lstvUserRole" layoutX="263.0" layoutY="110.0" prefHeight="237.0" prefWidth="327.0" />
    </children>
</AnchorPane>
